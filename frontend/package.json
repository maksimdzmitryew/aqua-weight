{
  "name": "aw-frontend",
  "private": true,
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "e2e": "playwright test --config ./playwright.config.ts",
    "e2e:headed": "playwright test --config ./playwright.config.ts --headed",
    "e2e:report": "playwright show-report ./playwright-report",
    "e2e:install": "npx playwright install --with-deps",
    "test:unit": "vitest --config ./vitest.config.ts",
    "test:unit:coverage": "vitest --config ./vitest.config.ts run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^3.0.4",
    "jsdom": "^25.0.1",
    "msw": "^2.6.9",
    "storybook": "^8.3.6",
    "vite": "^6.4.1",
    "vitest": "^3.0.4",
    "whatwg-fetch": "^3.6.20"
  }
}
